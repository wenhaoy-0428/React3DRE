{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "preserve",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": "./",
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "strict": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@/*": ["./src/*"],
      "@@/*": ["./src/.umi/*"],
      "@@test/*": ["./src/.umi-test/*"]
    }
  },
  "include": ["./**/*.d.ts", "./**/*.ts", "./**/*.tsx", "src/pages/Test/test.jsx", "src/pages/ShowModel/App.jsx", "src/components/Viser/ViewerWindow/ViewerWindow.jsx", "src/components/Viser/SidePanel/CameraPanel/CameraPanel.jsx", "src/components/Viser/Scene/Scene.jsx", "src/components/Scene/Scene.jsx"]
}
